var win = Ti.UI.createWindow({
	title:"Project4",
	modal:true,
	backgroundColor: "#333" 
});


var Gallery = Ti.UI.createButton({
	title : 'Gallery',
	height : 100,
	width : 200,
	top : 10,
	left : 10
});

var gPic = function(){
	var gWin = Ti.UI.createWindow({
		title: "Gallery",
		modal: true,
		backgroundColor: "#333"
	});
};

Gallery.addEventListener('click', gPic);

// Add to the parent view.
win.add(Gallery);
win.open();
